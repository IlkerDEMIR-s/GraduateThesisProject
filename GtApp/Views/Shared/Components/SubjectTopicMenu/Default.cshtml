@model IEnumerable<SubjectTopic>

<div class="mt-3">
    <h6 class="lead">
    <i class="fa fa-solid fa-bars text-secondary"></i>
        Subject Topics
    </h6>
</div>

<div class="my-4">
    <ul class="list-group">
        @foreach (var st in Model)
        {
            <li class="list-group-item">
                <a asp-action="Index" asp-route-SubjectTopicId = "@st.SUBJECT_TOPIC_ID">
                    @st.SUBJECT_TOPIC_CONTENT
                </a>
            </li>
        }
    </ul>
</div>